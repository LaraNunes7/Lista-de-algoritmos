
#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main() {
    // Inicializa o gerador de números aleatórios com o tempo atual
    srand(time(NULL));

    // Gera um número aleatório entre 1 e 100
    int numero_aleatorio = (rand() % 100) + 1;

    // Exibe o número gerado
    printf("Número aleatório gerado entre 1 e 100: %d\n", numero_aleatorio);

    return 0;
}
