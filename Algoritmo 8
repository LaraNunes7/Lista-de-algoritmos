#include <stdio.h>

int main() {
    int numero;

    // Ler o número para a tabuada
    printf("Digite um número para ver a tabuada: ");
    scanf("%d", &numero);

    // Exibir a tabuada do número
    printf("Tabuada do %d:\n", numero);
    for(int i = 1; i <= 10; i++) {
        printf("%d x %d = %d\n", numero, i, numero * i);
    }

    return 0;
}
